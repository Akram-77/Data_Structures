#include<stdio.h>
#include<stdlib.h>

int stack[100],i,top=-1,ch=0,n;
void push(){
    int val;
    if(top==n){
        printf("overflow occurs");
    }
    else{
        printf("enter the value");
        scanf("%d",&val);
        top=top+1;
        stack[top]=val;
    }
}
void pop(){
    if(top==-1)
        printf("under flow occurs");
    else
        top=top-1;
}
void display(){
    printf("the elements in array are:\n");
    for(i=top;i>=0;i--){
        printf("%d\n",stack[i]);
    }
    if(top==-1){
        printf("the stack is empty");
    }
}
void main(){
    
    printf("the number elements in the stack");
    scanf("%d",&n);
    while(ch!=4){
    printf("1.push 2.pop 3.display 4.exit\t");
    scanf("%d",&ch);
    switch(ch){
        case 1:
                push();
                break;
                
        case 2:
                printf("the last element is poped");
                pop();
                break;
                
        case 3:
                display();
                break;
        case 4:
                printf("existing");
                break;
    }
    }}
